
<<:tagging
   {
     :sch => $_.schedule,
     :date => $_.schedule.date.to_s,
     :from => $_.schedule.from ? $_.schedule.from.strftime("%H:%M"): "",
     :to => $_.schedule.to ? $_.schedule.to.strftime("%H:%M"): "",
   } <
  <<<-------------------------------------------------------------------------------
    |rowspan|| 5
    |       ||Schedule for <<:date>>||From:        || <<:from>>
  <<<-------------------------------------------------------------------------------
    |||To:          || <<:to>>
  <<<-------------------------------------------------------------------------------
    |||Preparation: || <<:sch \| :preparation>>
  <<<------------------------------------------------------------------------------
    |||Place:       || <<:sch \| :place>>
  <<<-------------------------------------------------------------------------------
    |||Memo:        || <<:sch \| :memo>>
